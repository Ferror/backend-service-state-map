{
    "organisation": {
        "token": "token",
        "name": "ferror"
    },
    "infoTypes": [
        {
            "identifier": "UUID1",
            "name": "php version",
            "description": "php version",
            "defaultBranch": "master",
            "defaultPath": "Dockerfile",
            "defaultScraper": "~PHP_VERSION=(\\d+.\\d+.\\d+)~"
        },
        {
            "identifier": "UUID2",
            "name": "composer version",
            "description": "composer version",
            "defaultBranch": "master",
            "defaultPath": "composer.json",
            "defaultScraper": "~COMPOSER_VERSION=(\\d+.\\d+.\\d+)~"
        },
        {
            "identifier": "UUID2",
            "name": "composer version",
            "description": "composer version",
            "defaultBranch": "master",
            "defaultPath": "composer.json",
            "defaultScraper": "-"
        }
    ],
    "services": [
        {
            "name": "sylius-standard",
            "description": "Sylius Standard Service",
            "infoSources": [
                {
                    "infoTypeId": "UUID1",
                    "branch": "master",
                    "path": "Dockerfile",
                    "scraper": "~PHP_VERSION=(\\d+.\\d+.*?)~"
                }
            ]
        },
        {
            "name": "warehouse",
            "description": "Warehouse Service",
            "infoSources": [
                {
                    "infoTypeId": "UUID1",
                    "branch": "master",
                    "path": "backend/Dockerfile",
                    "scraper": "~PHP_VERSION=(\\d+.\\d+.\\d+)~"
                },
                {
                    "infoTypeId": "UUID2",
                    "branch": "master",
                    "path": "backend/Dockerfile",
                    "scraper": "~COMPOSER_VERSION=(\\d+.\\d+.\\d+)~"
                },
                {
                    "infoTypeId": "UUID3",
                    "branch": "master",
                    "path": "backend/app/composer.json",
                    "scraper": "-"
                }
            ]
        }
    ]
}